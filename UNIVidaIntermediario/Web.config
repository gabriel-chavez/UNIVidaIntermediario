  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Remove ASPX Extension" stopProcessing="true">
          <match url="^(.*)\.aspx$" />
          <action type="Redirect" url="{R:1}" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
